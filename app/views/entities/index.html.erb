<p id="notice"><%= notice %></p>

<h1>Entities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Location</th>
      <th>Tel</th>
      <th>Cel</th>
      <th>Contactname</th>
      <th>Website</th>
      <th>Email</th>
      <th>Contactemail</th>
      <th>Contacttel</th>
      <th>Contactcel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entities.each do |entity| %>
      <tr>
        <td><%= entity.name %></td>
        <td><%= entity.address %></td>
        <td><%= entity.location %></td>
        <td><%= entity.tel %></td>
        <td><%= entity.cel %></td>
        <td><%= entity.contactname %></td>
        <td><%= entity.website %></td>
        <td><%= entity.email %></td>
        <td><%= entity.contactemail %></td>
        <td><%= entity.contacttel %></td>
        <td><%= entity.contactcel %></td>
        <td><%= link_to 'Show', entity %></td>
        <td><%= link_to 'Edit', edit_entity_path(entity) %></td>
        <td><%= link_to 'Destroy', entity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity', new_entity_path %>
